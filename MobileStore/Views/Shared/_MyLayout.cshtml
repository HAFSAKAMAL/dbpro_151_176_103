<!DOCTYPE html>
<html>
	<head>
		<title>@Page.Title</title>
		<title>Mobilestore</title>
		<link href=@Url.Content("/Content/css/style.css") rel="stylesheet" type="text/css"  media="all" />
		<meta name="keywords" content="Mobilestore iphone web template, Android web template, Smartphone web template, free webdesigns for Nokia, Samsung, LG, Sony Ericsson, Motorola web design" />
		<link href='//fonts.googleapis.com/css?family=Londrina+Solid|Coda+Caption:800|Open+Sans' rel='stylesheet' type='text/css'>
		<link rel="stylesheet" href=@Url.Content("/Content/css/responsiveslides.css")>
		<script src="@Url.Content("/Content/js/jquery.min.js")></script>
		<script src="@Url.Content("/Content/js/responsiveslides.min.js")></script>
		<script>
			// You can also use "$(window).load(function() {"
			$(function () {
			
				// Slideshow 1
				$("#slider1").responsiveSlides({
					maxwidth: 1600,
					speed: 600
				});
			});
			
			
		</script>
		@RenderSection("head", required: false)
	</head>
<body>
   
			
		
		<div class="top-header">
		<div class="wrap">
			<!----start-logo---->
			<div class="logo">
				<a href="index.html"><img src=@Url.Content("/Content/images/logo.png") title="logo" /></a>
			</div>
			<!----end-logo---->
			<!----start-top-nav---->
			<div class="top-nav">
				<ul>
					<li><a href=@Url.Content("/Home/Index")>Home</a></li>
					<li><a href=@Url.Content("/Home/News_")>News</a></li>
					<li><a href=@Url.Content("/Home/Outlet")>Outlet</a></li>
					@*<li><a href=@Url.Content("/Home/Reviews")>Reviews</a></li>*@
					<li><a href=@Url.Content("/Home/Contact")>Contact</a></li>
					<li><a href=@Url.Content("/Admins/Index")>Login</a></li>

				</ul>
			</div>
			<div class="clear"> </div>
		</div>
		</div>
		
		<!----End-top-nav---->
		<!----End-Header---->
		<!--start-image-slider---->
		
		<div class="clear"> </div>
		<div class="wrap">
		    <div class="content">
		       


		        <div>
		            @RenderBody()
		        </div>




		    </div>	
		</div>
		<div class="footer">
				<div class="wrap">
					<div class="section group">
						<div class="col_1_of_4 span_1_of_4">
							<h3>Latest-News</h3>
							<p>Find out news about latest and upcoming mobile phones.</p>
						</div>
						<div class="col_1_of_4 span_1_of_4">
							<h3>Outlet</h3>
							<p>Find out the outlet nearest to you here.</p>
					
						</div>
						<div class="col_1_of_4 span_1_of_4">
							<h3>Contact</h3>
							<p>Your queries/feedback is valued.</p>
					
						</div>
						<div class="col_1_of_4 span_1_of_4 footer-lastgrid">
					
					
							<h3>Follow Us:</h3>
							<ul>
								<li><a href="#"><img src=@Url.Content("/Content/images/twitter.png") title="twitter" />Twitter</a></li>
								<li><a href="#"><img src=@Url.Content("/Content/images/facebook.png") title="Facebook" />Facebook</a></li>
								<li><a href="#"><img src=@Url.Content("/Content/images/rss.png") title="Rss" />Rss</a></li>
							</ul>
						</div>
					</div>
				</div>
		
				<div class="clear"> </div>
				<div class="wrap">
					<div class="copy-right">
						<p>&copy; 2019 Mobile Store  <a href="">DBMS PROJRCT</a></p>
					</div>
				</div>
			</div>
	

</body>
